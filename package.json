{
  "name": "@examples/trpc-next-prisma-starter",
  "version": "10.44.1",
  "private": true,
  "browser": {
    "fs": false,
    "tls": false,
    "net": false
  },
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "pnpm prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "run-s test:*",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "pnpm generate"
  },
  "engines": {
    "node": ">=16.8"
  },
  "packageManager": "pnpm@8.5.0",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "proxy": "https://cdn.nftmania.io",
  "dependencies": {
    "@highlight-run/node": "^3.6.5",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/font": "^14.0.3",
    "@prisma/client": "^4.14.1",
    "@rainbow-me/rainbowkit": "^1.3.0",
    "@rainbow-me/rainbowkit-siwe-next-auth": "^0.3.4",
    "@reservoir0x/reservoir-sdk": "^1.12.3",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.44.1",
    "@trpc/next": "^10.44.1",
    "@trpc/react-query": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "@wagmi/cli": "^1.5.2",
    "aws-sdk": "^2.1536.0",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "cookies": "^0.8.0",
    "ethers": "^6.9.0",
    "next": "^14.0.1",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "siwe": "^2.1.4",
    "superjson": "^1.12.4",
    "viem": "^1.19.11",
    "wagmi": "^1.4.8",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@types/cookies": "^0.7.10",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.33",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.40.0",
    "eslint-config-next": "^14.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "prettier": "^2.8.8",
    "prisma": "^4.14.1",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.1.2",
    "vitest": "^0.32.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
